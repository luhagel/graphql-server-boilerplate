{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "member-access": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "interface-name": false,
    "no-console": false,
    "no-submodule-imports": false,
    "quotemark": true,
    "semicolon": false,
    "max-line-length": [
      true,
      120
    ]
  },
  "rulesDirectory": []
}